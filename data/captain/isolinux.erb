# Timeout is in units of 10ths of a second, so the 100 here means 10 seconds.
timeout <%= auto_install ? 1 : 100 %>
default <%= auto_install ? "install" : "normal" %>

label normal
  localboot -1

label install
  kernel /install/vmlinuz
  append file=/cdrom/install/preseed.seed initrd=/install/initrd.gz quiet locale=en_US console-setup/layoutcode=us netcfg/choose_interface=auto --
